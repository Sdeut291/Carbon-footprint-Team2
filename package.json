{
  "name": "carbon-footprint-team2",
  "version": "2.1.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:stats": "ng build --stats-json && npm run analyze",
    "build:compress": "gzipper compress ./dist/carbon-footprint-team2 ./dist/carbon-footprint-team2-gzip",
    "watch": "ng build --watch --configuration development",
    "analyze": "esbuild-visualizer --metadata ./dist/carbon-footprint-team2/stats.json --filename dist/analysis/index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:convert-coverage": "node .github/workflows/workflow-utils/convertJSONToMd.js",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "npx eslint",
    "eslint:fix": "npx eslint --fix",
    "lint-stage": "npx lint-staged -v",
    "stylelint": "stylelint ./src/**/*.{scss,sass,css}",
    "stylelint:fix": "stylelint ./src/**/*.{scss,sass,css} --fix",
    "prepare:husky": "npx husky init",
    "prepare": "husky",
    "format": "npx prettier . --check --ignore-unknown",
    "format:fix": "npx prettier . --write --ignore-unknown",
    "preplaywright": "npx playwright install",
    "playwright": "npx playwright test",
    "playwright:ui": "npm run playwright -- --ui",
    "audit": "npm audit --registry=https://registry.npmjs.org",
    "audit:fix": "npm audit fix --registry=https://registry.npmjs.org",
    "add-density": "ng add @bmw-fedev/density-components"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.4",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^18.2.4",
    "@angular/compiler": "^18.2.4",
    "@angular/core": "^18.2.4",
    "@angular/forms": "^18.2.4",
    "@angular/platform-browser": "^18.2.4",
    "@angular/platform-browser-dynamic": "^18.2.4",
    "@angular/router": "^18.2.4",
    "@aws-sdk/client-quicksight": "^3.699.0",
    "@bmw-ds/components": "^8.2.1",
    "amazon-quicksight-embedding-sdk": "^2.8.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.0",
    "zone.js": "~0.14.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^18.4.0",
    "@angular-eslint/eslint-plugin": "^18.4.0",
    "@angular-eslint/eslint-plugin-template": "^18.4.0",
    "@angular-eslint/schematics": "^18.4.0",
    "@angular-eslint/template-parser": "^18.4.0",
    "@angular/build": "^18.2.10",
    "@angular/cli": "~18.2.10",
    "@angular/compiler-cli": "^18.2.4",
    "@bmw-fedev/density-components": "^2.1.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-angular": "^19.5.0",
    "@playwright/test": "^1.48.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.1",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "esbuild-visualizer": "^0.6.0",
    "eslint": "^9.13.0",
    "gzipper": "^8.2.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "typescript": "^5.5.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.27.4"
  }
}
